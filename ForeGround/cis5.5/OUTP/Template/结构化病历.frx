    private void Text1_AfterData(object sender, EventArgs e)
    {                     
      Text1.Text=Text1.Text.Replace(&quot;复诊：否&quot;,&quot;&quot;);
      Text1.Text=Text1.Text.Replace(&quot;医师签全名&quot;,&quot;                           医师签全名&quot;);   
      Text1.Text=Text1.Text.Replace(&quot;接诊医生签名&quot;,&quot;                           接诊医生签名&quot;);
      
      //过滤为空的内容，使其不打印。通用
      string blnr=Text1.Text.Replace(&quot; &quot;,&quot;&quot;).Replace(&quot;\n&quot;,&quot;&quot;);
      //MessageBox.Show(blnr);
      string str1=&quot;*处方信息*检查检验措施或建议&quot;;
      bool yn=blnr.ToLower().Contains(str1.ToLower());
      if(yn)
      {
        Text1.Text=Text1.Text.Replace(&quot;*处方信息&quot;,&quot;&quot;);
      }
      str1=&quot;*检查检验措施或建议*休息方式或大概期限&quot;;
      yn=blnr.ToLower().Contains(str1.ToLower());
      if(yn)
      {
        Text1.Text=Text1.Text.Replace(&quot;*检查检验措施或建议&quot;,&quot;&quot;);
      }
      str1=&quot;*休息方式或大概期限*入院单信息&quot;;
      yn=blnr.ToLower().Contains(str1.ToLower());
      if(yn)
      {
        Text1.Text=Text1.Text.Replace(&quot;*休息方式或大概期限&quot;,&quot;&quot;);
      }
      str1=&quot;*入院单信息*传染病报告情况&quot;;
      yn=blnr.ToLower().Contains(str1.ToLower());
      if(yn)
      {
        Text1.Text=Text1.Text.Replace(&quot;*入院单信息&quot;,&quot;&quot;);
      }                          
      str1=&quot;*传染病报告情况医师签名&quot;;
      yn=blnr.ToLower().Contains(str1.ToLower());
      if(yn)
      {
        Text1.Text=Text1.Text.Replace(&quot;*传染病报告情况&quot;,&quot;&quot;);
      } 
      //儿科专用
      str1=&quot;*检查检验措施或建议*入院单信息&quot;;
      yn=blnr.ToLower().Contains(str1.ToLower());
      if(yn)
      {
        Text1.Text=Text1.Text.Replace(&quot;*检查检验措施或建议&quot;,&quot;&quot;);
      }
      str1=&quot;*入院单信息医师签名&quot;;
      yn=blnr.ToLower().Contains(str1.ToLower());
      if(yn)
      {
        Text1.Text=Text1.Text.Replace(&quot;*入院单信息&quot;,&quot;&quot;);
      }
      
      Text1.Text=Text1.Text.Replace(&quot;\n&quot;,&quot;br/&quot;);  
      Text1.Text=Text1.Text.Replace(&quot;br/br/&quot;,&quot;br/&quot;).Replace(&quot;br/br/&quot;,&quot;br/&quot;); 
      Text1.Text=Text1.Text.Replace(&quot;br/ br/&quot;,&quot;br/&quot;);    
      //MessageBox.Show(Text1.Text.ToLower().Contains(&quot;br/ br/&quot;.ToLower()).ToString());
      //MessageBox.Show(Text1.Text);
      Text1.Text=Text1.Text.Replace(&quot;br/&quot;,&quot;\n&quot;);
    }

    private void Page1_StartPage(object sender, EventArgs e)
    {
      if(Report.GetColumnValue(&quot;病历文件信息.病历名称&quot;).ToString().IndexOf(&quot;肠镜检查知情同意书&quot;)&gt;0
        ||Report.GetColumnValue(&quot;病历文件信息.病历名称&quot;).ToString().IndexOf(&quot;胃镜检查知情同意书&quot;)&gt;0
        ||Report.GetColumnValue(&quot;病历文件信息.病历名称&quot;).ToString().IndexOf(&quot;人工流产术后健康教育处方&quot;)&gt;0
        ||Report.GetColumnValue(&quot;病历文件信息.病历名称&quot;).ToString().IndexOf(&quot;药物流产健康教育处方&quot;)&gt;0)
      {
        Page1.TopMargin=10;
        Page1.BottomMargin=10;
        //Page1.LeftMargin=10;
        //Page1.RightMargin=10;
        Page1.PaperHeight=210;
        Page1.PaperWidth=297;
        string str=&quot;False&quot;;
        Page1.Landscape=bool.Parse(str);
      }
      if(Report.GetColumnValue(&quot;病历文件信息.病历名称&quot;).ToString().IndexOf(&quot;人工流产术后健康教育处方&quot;)&gt;0)
      {
        Text1.LineHeight=55F;
     
      }
      if(Report.GetColumnValue(&quot;病历文件信息.病历名称&quot;).ToString().IndexOf(&quot;药物流产健康教育处方&quot;)&gt;0)
      {
        Text1.LineHeight=36F;
     
      }
      
      if(Report.GetColumnValue(&quot;病历文件信息.病历名称&quot;).ToString().IndexOf(&quot;门诊“中心制”多学科联合诊疗记录&quot;)&gt;0)
      {
        
           Text15.Text=(&quot;门诊“中心制”多学科联合诊疗记录&quot;);
      }
      if(Report.GetColumnValue(&quot;病历文件信息.病历名称&quot;).ToString().IndexOf(&quot;中医门诊模板&quot;)&gt;0)
      {
        
        Text15.Text=(&quot;中医门诊病历&quot;);
      }
      if(Report.GetColumnValue(&quot;病历文件信息.病历名称&quot;).ToString().IndexOf(&quot;胃镜检查知情同意书&quot;)&gt;0)
      {
        
        Text15.Text=(&quot;胃镜检查知情同意书&quot;);
      }
      if(Report.GetColumnValue(&quot;病历文件信息.病历名称&quot;).ToString().IndexOf(&quot;肠镜检查知情同意书&quot;)&gt;0)
      {
        
        Text15.Text=(&quot;肠镜检查知情同意书&quot;);
         }
      if(Report.GetColumnValue(&quot;病历文件信息.病历名称&quot;).ToString().IndexOf(&quot;人工流产术后健康教育处方&quot;)&gt;0)
        {
        
          Text15.Text=(&quot;人工流产术后健康教育处方&quot;);
         }
      if(Report.GetColumnValue(&quot;病历文件信息.病历名称&quot;).ToString().IndexOf(&quot;药物流产健康教育处方&quot;)&gt;0)
          {
        
            Text15.Text=(&quot;药物流产健康教育处方&quot;);
         }
      }
    }
  }

</ScriptText>
