    <node refid="37" nodetype="Embeded" />
    <node text="不同意使用药物进行救治，对所发生的一切后果我们自行承担责任。" rtf="\b \'b2\'bb\'cd\'ac\'d2\'e2\b0\'ca\'b9\'d3\'c3\'d2\'a9\'ce\'ef\'bd\'f8\'d0\'d0\'be\'c8\'d6\'ce\'a3\'ac\'b6\'d4\'cb\'f9\'b7\'a2\'c9\'fa\'b5\'c4\'d2\'bb\'c7\'d0\'ba\'f3\'b9\'fb\'ce\'d2\'c3\'c7\'d7\'d4\'d0\'d0\'b3\'d0\'b5\'a3\'d4\'f0\'c8\'ce\'a1\'a3" nodetype="Text" />
    <node text="&#xA;    患者授权亲属签名：        与患者关系：      签名日期：     年    月    日" rtf="\par&#xD;&#xA;\pard     \'bb\'bc\'d5\'df\'ca\'da\'c8\'a8\'c7\'d7\'ca\'f4\'c7\'a9\'c3\'fb\'a3\'ba        \'d3\'eb\'bb\'bc\'d5\'df\'b9\'d8\'cf\'b5\'a3\'ba      \'c7\'a9\'c3\'fb\'c8\'d5\'c6\'da\'a3\'ba     \'c4\'ea    \'d4\'c2    \'c8\'d5" nodetype="Text" />
  </node>
  <node id="ef9d05fc-3382-4bb9-b157-29a8f601ef5c" nodetype="DynamicMoleNode" nodelogiccontent="" name="同意书与委托书(告知书内容)" text="同意书与委托书(告知书内容)" description="适用于告知书。" otherid="C-1192" display="20`医护人员陈述：`&#xD;&#xA;`0`1" permitobjs="" defaultobjs="" allowUserDefine="True" canNotInputText="False" isnewPage="False" canNull="True" canNullSubmit="True" MinLength="0" MaxLength="0" permittedDoctorGradeRange="" useDiagContinuedPrint="False" nodePrintArea="None" indentLength="0">
